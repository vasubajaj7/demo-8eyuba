{
  "environment": "dev",
  "gcp_project": "composer2-dev-project",
  "region": "us-central1",
  "zone": "us-central1-a",
  "dag_bucket": "composer2-dev-dags",
  "composer_environment": "composer2-dev",
  "airflow_version": "2.2.5",
  "composer_version": "2.0.0",
  "dag_processing_timeout": "60",
  "default_timezone": "UTC",
  "default_retry_delay": 300,
  "default_retries": 3,
  "postgres_conn_id": "postgres_default",
  "bq_conn_id": "bigquery_default",
  "gcs_conn_id": "google_cloud_default",
  "etl_source_prefix": "raw/",
  "etl_processed_prefix": "processed/",
  "etl_bq_dataset": "etl_data",
  "etl_bq_table": "processed_data",
  "alert_slack_channel": "#airflow-alerts",
  "monitoring_dashboard_url": "https://monitoring.googleapis.com/dashboard/composer2",
  "error_notification_emails": "airflow-admin@example.com,platform-support@example.com",
  "sensitive_data_handling": "encrypted",
  "max_active_runs_per_dag": 1,
  "airflow2_compatibility_mode": true,
  "environments": {
    "dev": {
      "gcp_project": "composer2-dev-project",
      "region": "us-central1",
      "zone": "us-central1-a",
      "dag_bucket": "composer2-dev-dags",
      "composer_environment": "composer2-dev",
      "etl_source_bucket": "etl-source-dev",
      "etl_target_bucket": "etl-target-dev",
      "alert_email": "dev-alerts@example.com",
      "source_data_path": "gs://etl-source-dev/data/"
    },
    "qa": {
      "gcp_project": "composer2-qa-project",
      "region": "us-central1",
      "zone": "us-central1-a",
      "dag_bucket": "composer2-qa-dags",
      "composer_environment": "composer2-qa",
      "etl_source_bucket": "etl-source-qa",
      "etl_target_bucket": "etl-target-qa",
      "alert_email": "qa-alerts@example.com",
      "source_data_path": "gs://etl-source-qa/data/"
    },
    "prod": {
      "gcp_project": "composer2-prod-project",
      "region": "us-central1",
      "zone": "us-central1-a",
      "dag_bucket": "composer2-prod-dags",
      "composer_environment": "composer2-prod",
      "etl_source_bucket": "etl-source-prod",
      "etl_target_bucket": "etl-target-prod",
      "alert_email": "prod-alerts@example.com",
      "source_data_path": "gs://etl-source-prod/data/"
    }
  },
  "dev_source_data_path": "gs://etl-source-dev/data/",
  "qa_source_data_path": "gs://etl-source-qa/data/",
  "prod_source_data_path": "gs://etl-source-prod/data/"
}